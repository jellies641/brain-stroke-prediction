# React App Configuration
REACT_APP_NAME=Brain Stroke Risk Prediction System
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_BASE_URL=http://localhost:3000

# Google OAuth Configuration (Optional)
# Get these from Google Cloud Console
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Feature Flags
REACT_APP_ENABLE_GOOGLE_AUTH=false
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PWA=false

# Analytics Configuration (Optional)
REACT_APP_GA_TRACKING_ID=GA_MEASUREMENT_ID
REACT_APP_GA_DEBUG=false

# Environment
REACT_APP_ENV=development

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false

# PWA Configuration
REACT_APP_PWA_NAME=Brain Stroke Predictor
REACT_APP_PWA_SHORT_NAME=Stroke Predictor
REACT_APP_PWA_DESCRIPTION=AI-powered stroke risk assessment tool

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#1976d2
REACT_APP_SECONDARY_COLOR=#dc004e

# Contact Information
REACT_APP_SUPPORT_EMAIL=support@strokepredictor.com
REACT_APP_SUPPORT_PHONE=+1-555-123-4567

# Social Media Links (Optional)
REACT_APP_GITHUB_URL=https://github.com/your-org/stroke-predictor
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/stroke-predictor
REACT_APP_TWITTER_URL=https://twitter.com/strokepredictor

# Legal Links
REACT_APP_PRIVACY_POLICY_URL=/privacy
REACT_APP_TERMS_OF_SERVICE_URL=/terms

# Security
REACT_APP_ENABLE_HTTPS=false
REACT_APP_STRICT_TRANSPORT_SECURITY=false

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_TIMEOUT=300000

# Development
REACT_APP_DEBUG=true
REACT_APP_MOCK_API=false

# External Services (Optional)
REACT_APP_SENTRY_DSN=your-sentry-dsn
REACT_APP_HOTJAR_ID=your-hotjar-id

# Localization (Future)
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr

# Medical Disclaimer Settings
REACT_APP_SHOW_MEDICAL_DISCLAIMER=true
REACT_APP_DISCLAIMER_TIMEOUT=10000

# Data Export Settings
REACT_APP_ENABLE_DATA_EXPORT=true
REACT_APP_MAX_EXPORT_RECORDS=1000

# UI Settings
REACT_APP_ITEMS_PER_PAGE=10
REACT_APP_ENABLE_DARK_MODE=false
REACT_APP_DEFAULT_THEME=light

# Validation Settings
REACT_APP_MIN_AGE=13
REACT_APP_MAX_AGE=120
REACT_APP_MIN_BMI=10
REACT_APP_MAX_BMI=60
REACT_APP_MIN_GLUCOSE=50
REACT_APP_MAX_GLUCOSE=500
